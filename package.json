    "adonis-fold": "^3.0.3",
    "adonis-framework": "^3.0.4",
    "adonis-lucid": "^3.0.10",
    "adonis-mail-provider": "^2.0.2",
    "adonis-middleware": "^1.0.8",
    "adonis-redis": "^1.0.1",
    "adonis-validation-provider": "^3.0.4",
    "ansi-html": "^0.0.6",
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
