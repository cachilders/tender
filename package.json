{
  "name": "tender",
  "version": "0.2.0",
  "main": "index.js",
  "author": "Casey A. Childers",
  "license": "MIT",
  "description": "Submission tracking for the detail obsessed.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cachilders/tender.git"
  },
  "bugs": {
    "url": "https://github.com/cachilders/tender/issues"
  },
  "homepage": "https://github.com/cachilders/tender#readme",
  "engines": {
    "node": "6.3.0"
  },
  "scripts": {
    "dev": "nodemon --watch app --watch bootstrap --watch config --watch .env -x node --harmony_proxies server.js",
    "start": "npm run build && node --harmony_proxies server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config config/webpack/client.config.js --progress --hide-modules",
    "build:server": "webpack --config config/webpack/server.config.js --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src app/{**,**/**}/*.js tests/{*,**/*}.js"
  },
  "autoload": {
    "App": "./app"
  },
  "dependencies": {
    "adonis-ace": "^3.0.4",
    "adonis-ally": "^1.0.0",
    "adonis-auth": "^1.0.4",
